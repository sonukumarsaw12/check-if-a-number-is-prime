#include<iostream>
using namespace std;

int main(){
    int n;
    
    cout<<"Enter n :";
    cin >> n;
    
    for(int i=2;i<=n-1;i++){
        if(n % i==0){
            cout<<"prime number" << endl;
        }
        
        else{
        cout<<"not prime number" << endl;
        }
        
        break;
    
    }
    if(n==0){
        cout<<"not prime number";
    }
    
}
